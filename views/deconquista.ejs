            </svg></i></a>
      </div>
      <div class="deconquista-page">
        <div class="container">
          <div class="title-para">
            <h1 class="about-sio-title" id="deconquista-header">
              DeConquista
            </h1>
          </div>
          <% deconquistaData.forEach(deconquista=> { %>
            <div class="deconquista-img">
              <img src="<%= `https://event-manager.syd1.cdn.digitaloceanspaces.com/${ deconquista.image}` %>"
                alt="DeConquista" loading="lazy" />
            </div>
            <div class="deconquista-desc">
              <p>
                <%= deconquista.description %>
              </p>
            </div>
            <% }); %>
              <!-- <div class="conquista-btn">
                <button class="btn event-primary" type="submit">Know More <i class="fa fa-arrow-right"></i></button>
              </div> -->
              <!-- ========== Start leader notes slider ========== -->
              <div class="leader_notes_header">
                <div class="title-para">
                  <h1 class="about-sio-title">Leadersâ€™ Notes</h1>
                </div>
                <div class="blog-slider">
                  <div class="blog-slider__wrp swiper-wrapper">
                    <% leadersNoteData.forEach(data=> { %>
                      <div class="blog-slider__item swiper-slide">
                        <div class="blog-slider__img">
                          <img src="<%= `https://event-manager.syd1.cdn.digitaloceanspaces.com/${ data.image }` %>"
                            alt="" loading="lazy" />
                        </div>
                        <div class="blog-slider__content">
                          <div class="blog-slider__title_1">
                            <%= data.name %>
                          </div>
                          <div class="blog-slider__title_2">
                            <%= data.designation %>
                          </div>
                          <div class="blog-slider__text">
                            <%= data.description %>
                          </div>
                        </div>
                      </div>
                      <% }); %>

                  </div>
                  <div class="blog-slider__pagination"></div>
                </div>
              </div>
              <!-- ========== End leader notes slider ========== -->
              <!-- ========== Start speaker section ========== -->
              <div class="speaker_deconquista">
                <!-- <div class="rectangle_background_down"></div> -->
                <div class="title-para">
                  <h1 class="about-sio-title">Speakers</h1>
                </div>
                <div class="deconquista-slider">
                  <div class="slide-container swiper">
                    <div class="slide-content">
                      <div class="card-wrapper swiper-wrapper">
                        <% speakerData.forEach(speaker=> { %>
                          <div class="card-speakers swiper-slide">
                            <div class="image-content">
                              <span class="overlay"></span>
                              <div class="card-image-items">
                                <img
                                  src="<%= `https://event-manager.syd1.cdn.digitaloceanspaces.com/${ speaker.photo }` %>"
                                  alt="" class="card-img" loading="lazy" />
                              </div>
                            </div>
                            <div class="card-content">
                              <h2 class="name">
                                <%= speaker.name %>
                              </h2>
                              <p class="description">
                                <%= speaker.designation %>
                              </p>
                            </div>
                          </div>
                          <% }); %>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!-- ========== End speaker section ========== -->
        </div>
      </div>

