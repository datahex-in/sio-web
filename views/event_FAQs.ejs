                            <ul style="list-style-type: disc; color: white; padding-top: 10px;">
                                <li><%= item.question %></li>
                                <p><%= item.answer %></p>
                            </ul>
                            <% }); %>
                        </div>
                    </div>
                </div>
            </div>

    </div>
    <%-include('mainFooter.ejs')%>

